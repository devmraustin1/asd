    <script>
    $(document).on('click', '.miningBtn', function (e) {
        e.preventDefault();
        $.ajax({
            type: "post",
            url: "https://coin-crest.icu/mining-on",
            success: function (res) {
                notifyMsg(res.msg, res.cls);
                $('#miningModal').modal('hide');
                $('.modal-backdrop').removeClass('modal-backdrop');
                if (res.cls == 'success') {
                    setTimeout(() => {
                        location.reload();
                    }, 2200);
                }
            }
        });

    });
</script>
<script>
    $(function () {
        'use strict'

        $('.buy-win').on('click', function () {
            const modal = $('#win-modal');
            modal.modal('show')
        })

        $('.buy-loose').on('click', function () {
            const modal = $('#loose-modal');
            modal.modal('show')
        })
        
        
    });
    
    
    function setPeriod(pr) {
        
        $('#setP1').removeClass('disabled');
        $('#setP3').removeClass('disabled');
        $('#setP9').removeClass('disabled');
        
        $('#setP'+pr).addClass('disabled');
        $('#period').val(pr);
        
        updateProfit();
        
    };
    
    function setAmount(am) {
        
        $('#setA100').removeClass('disabled');
        $('#setA200').removeClass('disabled');
        $('#setA500').removeClass('disabled');
        $('#setA1000').removeClass('disabled');
        $('#setA2000').removeClass('disabled');
        
        $('#setA'+am).addClass('disabled');
        $('#amount').val(am);
        $('#betAmount').html("Money <br> "+ am + " USDT");
        
        updateProfit();
        
    };
    
    function updateProfit() {
        var prv = $('#period').val();
        var prc = 0;
        var am = $('#amount').val();
        
        
        if(prv == "1") {
            prc = 7;
        } else if(prv == 3) {
            prc = 23;
        } else if(prv == 9) {
            prc = 75;
        }
        
        var prf = (prc/100) * am;
        
        $('#profit').val(prf);
        $('#betProfit').html("Profit <br> "+ prf.toFixed(2) + " USDT");
    }
    
    
    
    function LsetPeriod(pr) {
        
        $('#LsetP1').removeClass('disabled');
        $('#LsetP3').removeClass('disabled');
        $('#LsetP9').removeClass('disabled');
        
        $('#LsetP'+pr).addClass('disabled');
        $('#Lperiod').val(pr);
        
        LupdateProfit();
        
    };
    
    function LsetAmount(am) {
        
        $('#LsetA100').removeClass('disabled');
        $('#LsetA200').removeClass('disabled');
        $('#LsetA500').removeClass('disabled');
        $('#LsetA1000').removeClass('disabled');
        $('#LsetA2000').removeClass('disabled');
        
        $('#LsetA'+am).addClass('disabled');
        $('#Lamount').val(am);
        $('#LbetAmount').html("Money <br> "+ am + " USDT");
        
        LupdateProfit();
        
    };
    
    function LupdateProfit() {
        var prv = $('#Lperiod').val();
        var prc = 0;
        var am = $('#Lamount').val();
        
        
        if(prv == "1") {
            prc = 7;
        } else if(prv == 3) {
            prc = 23;
        } else if(prv == 9) {
            prc = 75;
        }
        
        var prf = (prc/100) * am;
        
        $('#Lprofit').val(prf);
        $('#LbetProfit').html("Profit <br> "+ prf.toFixed(2) + " USDT");
    }
    
    
    $(document).on('click', '#submit', function (e) {
        var amount = $('#amount').val();
        var coin = $('#coin').val();
        var period = $('#period').val();
        var type = $('#type').val();
        var profit = $('#profit').val();
        e.preventDefault();
        $.ajax({
            type: "post",
            url: "/bet/store",
            data: {
                amount, period, type, profit, coin
            },
            success: function (res) {
                notifyMsg(res.msg, res.cls);
                $('#win-modal').modal('hide');
                if (res.cls == 'success') {
                    setTimeout(() => {
                        location.replace('/bet-result/'+res.bet.id);
                    }, 2200);
                }
            }
        });

    });
    
    
    $(document).on('click', '#Lsubmit', function (e) {
        var amount = $('#Lamount').val();
        var coin = $('#Lcoin').val();
        var period = $('#Lperiod').val();
        var type = $('#Ltype').val();
        var profit = $('#Lprofit').val();
        e.preventDefault();
        $.ajax({
            type: "post",
            url: "/bet/store",
            data: {
                amount, period, type, profit, coin
            },
            success: function (res) {
                notifyMsg(res.msg, res.cls);
                $('#loose-modal').modal('hide');
                if (res.cls == 'success') {
                    setTimeout(() => {
                        location.replace('/bet-result/'+res.bet.id);
                    }, 2200);
                }
            }
        });

    });
</script>

