    <script>

        $(document).on('click', '.customLink', function (e) {
            e.preventDefault();
            let hrefLink = $(this).attr('href');
            $('#preLoadCustom').removeClass('d-none'); // loader-on
            setTimeout(() => {
                GoTo(hrefLink)
            }, 100);
        });



